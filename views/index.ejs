
 <%- include("partials/header") -%>
 
 <div class="container text-center">
  <div class="row">
    <div class="col">
      1 of 2
      
      <% products.forEach(product => {%> 
      <img src="<%= product && product.image && product.image.url %>" class="img-fluid" alt="...">
